<%- fields_for "multi_objs[]", iform do |f| -%>
  <tr class="iform">
    <td><%= f.text_field :issue, :size => 12 %></td>
    <td><%= f.text_field :description %></td>
    <td><%= f.text_field :url, :size => 15 %></td>
    <td><%= f.select(:severity, severity_array) %></td>
    <td><%= f.text_field :owner, :size => 15 %></td>
    <td><%= f.text_field :user, :size => 15 %></td>
    <td>
      <%= check_box_tag 'multi_objs[][resolved]', :true, check_box_value(i||=0, :resolved) %>
      <%= status_icon(true) %>
    </td>
    <td colspan="3">
      <%= link_to_function 'Remove', "$(this).up('.iform').remove();" %>
    </td>
  </tr>
<%- end -%>
